/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=or12);
    Or(a=or12, b=in[2], out=or123);
    Or(a=or123, b=in[3], out=or1234);        
    Or(a=or1234, b=in[4], out=or12345);
    Or(a=or12345, b=in[5], out=or123456);    
    Or(a=or123456, b=in[6], out=or1234567);
    Or(a=or1234567, b=in[7], out=out);        
        // Put your code here:
}